# Redirigir facebook-feed-final.xml a facebook-feed-final.php
RewriteEngine On
RewriteRule ^facebook-feed-final\.xml$ facebook-feed-final.php [L]

# Configuraci√≥n para archivos XML
<Files "facebook-feed-final.php">
    Header set Content-Type "text/xml; charset=utf-8"
    Header set Cache-Control "no-cache, no-store, must-revalidate"
    Header set Pragma "no-cache"
    Header set Expires "0"
</Files>

# Asegurar que los archivos XML se sirvan correctamente
AddType application/xml .xml
AddDefaultCharset UTF-8